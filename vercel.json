{
  "version": 2,
  "builds": [
    {
      "src": "app-consultor/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist/app-consultor" }
    }
  ],
  "outputDirectory": "app-consultor/dist/app-consultor",
  "routes": [
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
